{
  "name": "ecolearnia-interactives",
  "version": "0.0.2",
  "description": "EcoLearnia's interactives (UI) library",
  "main": "el-interactives.js",
  "scripts": {
    "webserver": "python -m SimpleHTTPServer 8000",
    "opentest": "open /Applications/Google\\ Chrome.app http://localhost:8000/artifacts/samples/sample-interactives1.html",
    "build-deps": "browserify -t babelify  -r bluebird -r lodash -r react/addons | uglifyjs -c >  build/el-externdeps.bundle.min.js",
    "build-inter": "browserify -t [ babelify --presets [ es2015 react ] ] -x bluebird -x lodash -x react/addons -r ./src/interactives.js:interactives -r -o build/el-interactives.bundle.js -d",
    "watch-inter": "watchify   -t [ babelify --presets [ es2015 react ] ] -x bluebird -x lodash -x react/addons -r ./src/interactives.js:interactives -r -o build/el-interactives.bundle.js -d -v",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register -u tdd --recursive --reporter spec test/spec --timeout 1000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altenia/ecolearnia-interactives.git"
  },
  "keywords": [
    "Widget",
    "user",
    "interface"
  ],
  "author": "Young-Suk Ahn Park",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/altenia/ecolearnia-interactives/issues"
  },
  "homepage": "https://github.com/altenia/ecolearnia-interactives#readme",
  "dependencies": {
    "bluebird": "^3.2.2",
    "immutable": "^3.7.6",
    "lodash": "^4.3.0",
    "mathjs": "^2.7.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^6.3.0",
    "blanket": "^1.1.7",
    "browserify": "^11.2.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.0",
    "mocha": "^2.4.5",
    "should": "^7.1.1",
    "sinon": "^1.17.3",
    "watchify": "^3.4.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
